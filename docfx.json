{
    "metadata": [
        {
            "src": [
                {
                    "files": [
                        "Microsoft.Health.Fhir.sln",
                        "src/**/*.csproj"
                    ],
                    "exclude": [
                        "**/bin/**",
                        "**/obj/**"
                    ]
                }
            ],
            "dest": "obj/api"
        }
    ],
    "build": {
        "content": [
            {
                "files": [
                    "**/*.yml"
                ],
                "src": "obj/api",
                "dest": "api"
            },
            {
                "files": [
                    "*.md",
                    "toc.yml"
                ],
                "src": "docs",
                "dest": "articles"
            },
            {
                "files": [
                    "index.md",
                    "toc.yml"
                ]
            }
        ],
        "resource": [
            {
                "files": [
                    "docs/images/**",
                    "src/**/*.json"
                ]
            }
        ],
        "overwrite": [
            {
                "files": [
                    "docs/apidoc/**.md"
                ],
                "exclude": [
                    "obj/**",
                    "_site/**"
                ]
            }
        ],
        "dest": "_site",
        "globalMetadataFiles": [],
        "fileMetadataFiles": [],
        "template": [
            "default"
        ],
        "postProcessors": [],
        "noLangKeyword": false
    }
}
